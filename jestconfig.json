{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*", "!src/content/**/*", "!src/models/**/*", "!src/example/**/*"]
}